	repositoryformatversion = 0
	fileMode = false
	bare = false
	logallrefupdates = true
	ignorecase = true
	autocrlf = input
	safecrlf = false
[remote "origin"]
	url = git@github.com:fobiaweb/fobia.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "develop"]
	remote = origin
	merge = refs/heads/develop
[filter "dater"]
	smudge = expand_date
	clean = perl -pe \"s/\\\\\\$Date[^\\\\\\$]*\\\\\\$/\\\\\\$Date\\\\\\$/\"
